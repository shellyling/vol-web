{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/_pgbackup": true,
    ".nuxt": true,
    ".output": true,
    "dist": true,
    ".unlighthouse": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#5A093B",
    "titleBar.activeBackground": "#7E0C53",
    "titleBar.activeForeground": "#FFFCFD"
  }
}